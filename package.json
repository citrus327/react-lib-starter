{
  "name": "a-priceless-react-lib-tester",
  "version": "0.1.0",
  "description": "my react lib starter",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "dev": "rimraf dist && cross-env NODE_ENV=development run-p -l \"build:lib -w\" \"build:declaration --watch\"",
    "build": "rimraf dist && cross-env NODE_ENV=production run-p build:lib build:declaration",
    "build:lib": "rollup -c rollup.config.mjs",
    "build:declaration": "tsc",
    "docs": "docit start docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/citrus327/react-lib-starter.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/citrus327/react-lib-starter/issues"
  },
  "homepage": "https://github.com/citrus327/react-lib-starter#readme",
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@blizzbolts/docit": "^0.11.0",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-url": "^8.0.1",
    "@swc/core": "^1.3.72",
    "@tsconfig/create-react-app": "^2.0.1",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-swc3": "^0.9.1",
    "typescript": "5.0.4"
  }
}
